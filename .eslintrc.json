{
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": { "project": ["./tsconfig.json"] },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "no-unused-vars": "error",
        "no-var": "error",
        "no-empty-function": "error",
        "eol-last": ["error", "always"]
    }
}
